{
  "name": "nextgen-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "clean": "rm -rf node_modules package-lock.json dist",
    "clean-install": "npm run clean && npm install",
    "start": "node ./dist/bin/www.js",
    "dev": "cross-env NODE_ENV=development ts-node ./bin/www.ts",
    "dev:hot": "nodemon --watch ./src --ext ts,js --exec \"npm run dev\"",
    "lint": "eslint . --ext .ts,.js",
    "start-prod": "cross-env NODE_ENV=production node ./dist/bin/www.js",
    "type-check": "tsc --noEmit",
    "prestart": "npm run build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "ts-node --project tsconfig.json prisma/seed.ts",
    "swagger:generate": "ts-node src/swagger.ts",
    "swagger:watch": "nodemon --watch src/routes --watch src/controllers --ext ts --exec \"npm run swagger:generate\"",
    "swagger:dev": "concurrently \"npm run swagger:watch\" \"npm run dev:hot\""
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@faker-js/faker": "^9.9.0",
    "@prisma/internals": "^6.11.1",
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-validator": "^7.2.1",
    "fontkit": "^2.0.4",
    "http-errors": "~1.6.3",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^7.0.3",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.20",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "tsoa": "^6.6.0",
    "uuid": "^11.1.0",
    "wkx": "^0.5.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@prisma/client": "^6.13.0",
    "@quramy/prisma-fabbrica": "^2.3.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/core": "^1.11.24",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/debug": "^4.1.12",
    "@types/ejs": "^3.1.5",
    "@types/find": "^0.2.4",
    "@types/fontkit": "^2.0.8",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "^2.0.4",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.16",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfmake": "^0.2.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.26.0",
    "eslint-plugin-n": "^17.17.0",
    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.2.4"
  }
}
